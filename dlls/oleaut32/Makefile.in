MODULE    = oleaut32.dll
IMPORTLIB = oleaut32
IMPORTS   = uuid ole32 rpcrt4 user32 gdi32 advapi32
DELAYIMPORTS = comctl32 urlmon windowscodecs
EXTRADEFS = -D_OLEAUT32_

EXTRADLLFLAGS = -mno-cygwin

C_SRCS = \
	connpt.c \
	dispatch.c \
	hash.c \
	oleaut.c \
	olefont.c \
	olepicture.c \
	olepropframe.c \
	recinfo.c \
	safearray.c \
	typelib.c \
	usrmarshal.c \
	varformat.c \
	variant.c \
	vartype.c

RC_SRCS = oleaut32.rc

IDL_SRCS = \
	oleaut32_oaidl.idl \
	oleaut32_ocidl.idl

dlldata_EXTRADEFS = -DENTRY_PREFIX=OLEAUTPS_ -DPROXY_DELEGATION -DWINE_REGISTER_DLL -DPROXY_CLSID=CLSID_PSFactoryBuffer
